<?xml version="1.0" encoding="UTF-8"?>
<xml>
  <file>
    <title>OriginalLength</title>
    <breadcumbs>
      <link file="BitMiracle_LibJpeg_Classic.xml">
        <text>BitMiracle.LibJpeg.Classic namespace</text>
      </link>
      <text> :: </text>
      <link file="jpeg_marker_struct.xml">
        <text>jpeg_marker_struct</text>
      </link>
    </breadcumbs>
    <descriptionarticle only="false">
      <text>Full length of original data associated with marker.</text> </descriptionarticle>
    <signature>
      <property access="read">
        <name>OriginalLength</name>
        <return type="Int32" />
      </property>
    </signature>
    <remarks>
      <text>This length excludes the marker length word, whereas the stored representation within the JPEG file includes it. (Hence the maximum data length is really only 65533.)</text> </remarks>
  </file>
</xml>