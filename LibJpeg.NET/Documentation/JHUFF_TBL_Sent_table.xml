<xml>
  <file>
    <title>Sent_table</title>
    <breadcumbs>
      <link file="BitMiracle_LibJpeg_Classic.xml">
        <text>BitMiracle.LibJpeg.Classic namespace</text>
      </link>
      <text> :: </text>
      <link file="JHUFF_TBL.xml">
        <text>JHUFF_TBL</text>
      </link>
    </breadcumbs>
    <descriptionarticle only="false">
      <text>Indicates whether the table has been output to file.</text> </descriptionarticle>
    <seealso>
      <ref caption="jpeg_suppress_tables" file="jpeg_compress_struct.jpeg_suppress_tables.xml" />
    </seealso>
    <signature>
      <property access="full">
        <name>Sent_table</name>
        <return type="Boolean">
          <text>It's initialized </text> <b><text>false</text> </b><text> when the table is created, and set </text> <b><text>true</text> </b><text> when it's been output to the file. You could suppress output of a table by setting this to </text> <b><text>true</text> </b><text>.</text> </return>
      </property>
    </signature>
    <remarks>
      <text>This property is used only during compression.</text> </remarks>
  </file>
</xml>