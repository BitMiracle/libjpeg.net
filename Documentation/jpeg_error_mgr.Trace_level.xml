<?xml version="1.0" encoding="UTF-8"?>
<xml>
  <file>
    <title>Trace_level</title>
    <breadcumbs>
      <link file="BitMiracle_LibJpeg_Classic.xml">
        <text>BitMiracle.LibJpeg.Classic namespace</text>
      </link>
      <text> :: </text>
      <link file="jpeg_error_mgr.xml">
        <text>jpeg_error_mgr</text>
      </link>
    </breadcumbs>
    <descriptionarticle only="false">
      <text>Maximum message level that will be displayed.</text> </descriptionarticle>
    <seealso>
      <ref caption="emit_message" file="jpeg_error_mgr.emit_message.xml" />
    </seealso>
    <signature>
      <property access="full">
        <name>Trace_level</name>
        <return type="Int32">
          <text>Values are:</text> <br /><text>-1: recoverable corrupt-data warning, may want to abort.</text> <br /><text>0: important advisory messages (always display to user).</text> <br /><text>1: first level of tracing detail.</text> <br /><text>2, 3, ...: successively more detailed tracing messages.</text> </return>
      </property>
    </signature>
  </file>
</xml>